{
  "extends": [
    "tslint:recommended"
  ],
  "cliOptions": {
    "exclude": [
      "**/node_modules/**/*"
    ]
  },
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single", "avoid-escape"],
    "indent": [true, "spaces", 2],
    "max-line-length": [true, 160],
    "object-literal-sort-keys": false
  },
  "rulesDirectory": []
}